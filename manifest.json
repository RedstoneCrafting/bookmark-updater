{
    "manifest_version": 2,
    "browser_specific_settings": {
        "gecko": {
            "id": "bookmark-updater@tim0est"
        }
    },
    "name": "Bookmark Updater",
    "version": "1.1",
    "description": "A simple Add-on that adds the ability to update bookmarks",
    "icons": {
        "48": "icons/48.png"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "main.js"
        ]
    }
}